	public boolean largerTotalCostThan(Purchase p1, Purchase p2) {
			if(p1.getTotalCost() < p2.getTotalCost()) {
				return true ;
			}
			else if (p2.getTotalCost() < p1.getTotalCost()) {
				return true; 
			}
			return false; 
		}
