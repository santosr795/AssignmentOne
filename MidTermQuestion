		double Sum = numberOne + numberTwo; 
		double Average = Sum/AverNum; 
		System.out.println("The Average of " + numberOne +" and " + "is equals to " + Average);
		System.out.println("Do you want to find add other number for \n find the Average. Enter Yes or No. ");
			sentinel = keyboard.nextLine();	
		while(sentinel.equalsIgnoreCase("Yes")) {
			
			AverNum= AverNum++; 
			System.out.println("Enter another number to add");
			numberTwo = keyboard.nextDouble(); 
			Sum = Sum + numberTwo; 
			Average = Sum / AverNum;
			System.out.println("The average is " + Average);
	
			System.out.println("Do you want to find add other number for find the Average. Enter Yes or No. ");
