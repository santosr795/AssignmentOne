					public void changeGroupPrice( double salePercent ) {
					salePercent = groupPrice; 
					if(salePercent > 0 && salePercent < .5) {
						groupPrice = salePercent; 
					}
					else {
						System.out.println("That's out of the sale percentage is out of range.");
						System.exit(0);
					}
		
